{
    "window.menuBarVisibility": "toggle",
    "workbench.tree.indent": 16,
    "files.trimTrailingWhitespace": true,
    "workbench.startupEditor": "newUntitledFile",
    "explorer.compactFolders": false,
    "rest-client.requestNameAsResponseTabTitle": true,
    "rest-client.rememberCookiesForSubsequentRequests": false,
    "rest-client.decodeEscapedUnicodeCharacters": true,
    "rest-client.previewOption": "exchange",
    "rest-client.previewResponsePanelTakeFocus": false,
    "editor.tabSize": 2,
    "editor.rulers": [
        80,
        120
    ],
    "redhat.telemetry.enabled": false,
    "yaml.customTags": [
        "!Equals sequence",
        "!FindInMap sequence",
        "!GetAtt",
        "!GetAZs",
        "!ImportValue",
        "!Join sequence",
        "!Ref",
        "!Select sequence",
        "!Split sequence",
        "!Sub",
        "!If sequence",
        "!Not sequence",
        "!Condition sequence",
        "!Or sequence"
    ]
}